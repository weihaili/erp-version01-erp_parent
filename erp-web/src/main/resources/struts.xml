<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
    <package name="default" namespace="/" extends="struts-default">
    	<action name="dep_*" class="depAction" method="{1}"></action>
    	
    	<action name="emp_*" class="empAction" method="{1}"></action>
    	
    	<action name="goodsType_*" class="goodsTypeAction" method="{1}"></action>
    	
    	<action name="goods_*" class="goodsAction" method="{1}"></action>
    	
    	<action name="store_*" class="storeAction" method="{1}"></action>
    	
    	<action name="inventory_*" class="inventoryAction" method="{1}"></action>
    	
    	<action name="menu_*" class="menuAction" method="{1}"></action>
    	
    	<action name="login_*" class="loginAction" method="{1}"></action>
    	
    	<action name="role_*" class="roleAction" method="{1}"></action>
    	
    	<action name="empRole_*" class="empRoleAction" method="{1}"></action>
    	
    	<action name="order_*" class="orderAction" method="{1}"></action>
    	
    	<action name="supplier_*" class="supplierAction" method="{1}"></action>
    	
    	<action name="orderDetail_*" class="orderDetailAction" method="{1}"></action>
    	
    	<action name="returnOrder_*" class="returnOrderAction" method="{1}"></action>
    	
    	<action name="returnOrderDetail_*" class="returnOrderDetailAction" method="{1}"></action>
    	
    	<action name="storeDetail_*" class="storeDetailAction" method="{1}"></action>
    	
    	<action name="storeOper_*" class="storeOperAction" method="{1}"></action>
    	
    	<action name="roleMenu_*" class="roleMenuAction" method="{1}"></action>
    	
    	<action name="report_*" class="reportAction" method="{1}"></action>
    </package>
    
</struts>
